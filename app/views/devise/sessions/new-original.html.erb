<h2>Iniciar Sesi√≥n</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

  <div class="field">
    Run <br>
    <%= f.text_field :login, autofocus: true, :size=>"10" , :maxlength=>"8"%>
    - <%= f.text_field :run_dig_ver, :size=>"2", :maxlength=>"1", placeholder:" k"%>
  </div>

  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off" ,:size=>"15" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
